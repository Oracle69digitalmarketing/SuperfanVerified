{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
"production": {
  "env": {
    "EXPO_PUBLIC_ENV": "production"
  },
  "expo": {
    "version": "52.0.0"
  },
  "android": {
    "buildType": "app-bundle"
  },
  "ios": {
    "buildType": "archive"
  },
  "web": {
    "buildCommand": "expo export:web",
    "outputDir": "web-build"
  }
}
